#!/bin/sh
#export NO_AT_BRIDGE=1
#export QT_ACCESSIBILITY=0
#export QT_LINUX_ACCESSIBILITY_ALWAYS_ON=0
ogg123 /usr/share/sounds/ubuntu/stereo/service-login.ogg &
export SSH_ASKPASS=/usr/local/bin/e-askpass.sh
eval  `ssh-agent -s`
. $HOME/.bash_profile
setxkbmap -option ctrl:nocaps
xmodmap $HOME/.xmodmap 
xcape -e 'Control_L=Escape'
pidof ibus && ibus exit
xrdb -merge $HOME/.Xresources &
xautolock -detectsleep -time 60  -locker  "$HOME/bin/xlock" &
emacs  -title Emacs &
xterm  -title Retriever &
export LISP=sbcl
${HOME}/bin/export_x_info &
dbus-launch --exit-with-session /usr/bin/stumpwm
#dbus-launch --exit-with-session emacs 
 #exec gnome-session
